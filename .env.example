# Blockchain Configuration
EVM_RPC_URL=https://galactica-cassiopeia.g.alchemy.com/public

# Configuration file path (defaults to config.yaml)
# For Cassiopeia testnet: CONFIG_FILE=config/cassiopeia.yaml
# For Mainnet: CONFIG_FILE=config/mainnet.yaml
CONFIG_FILE=config/cassiopeia.yaml

# Private key for queue processor transactions
# WARNING: Never commit real private keys to version control!
# Generate a new key for production use
PRIVATE_KEY=0xYOUR_PRIVATE_KEY_HERE

# Server Configuration (optional)
# PORT=8080

# Merkle Proof Service Configuration (optional)
# Defaults to grpc-merkle.lookhere.tech:443 with TLS enabled
MERKLE_SERVICE_URL=grpc-merkle.lookhere.tech:443
MERKLE_SERVICE_TLS=true